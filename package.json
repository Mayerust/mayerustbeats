{
  "name": "mayerustbeats",
  "version": "1.0.0",
  "description": "Retro Nintendo-style beat selling portfolio by Mayerust",
  "main": "index.html",
  "scripts": {
    "build": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build-prod": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "dev": "live-server --port=3000 --entry-file=index.html",
    "deploy": "npm run build-prod && vercel"
  },
  "keywords": [
    "beats",
    "music",
    "portfolio",
    "retro",
    "nintendo"
  ],
  "author": "Mayerust",
  "license": "ISC",
  "devDependencies": {
    "live-server": "^1.2.2",
    "tailwindcss": "^3.4.0"
  }
}
